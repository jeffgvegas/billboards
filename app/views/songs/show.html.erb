<p id="notice"><%= notice %></p>

<h1>Song Info</h1>
<table>
  <tr>
    <td>Title:</td>
    <td><%= @song.title %></td>
  </tr>
  <tr>
    <td>Release Date:</td>
    <td><%= @song.released_on %></td>
  </tr>
  <tr>
    <td>Genre:</td>
    <td><%= @song.genre %></td>
  </tr>
  <tr>
    <td>Rating:</td>
    <td><%= @song.rating.to_i %></td>
  </tr>
  <tr>
    <td>Artist Id:</td>
    <td><%= @song.artist_id %></td>
  </tr>
    <td><%= link_to 'Edit', edit_song_path(@song) %></td>
    <td><%= link_to 'Back', songs_path %></td>
  </tr>
</table>